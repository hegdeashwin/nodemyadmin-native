{
  "name": "nodemyadmin",
  "version": "0.0.1",
  "description": "A tool intended to handle the administration of MySQL over the mobile as native app for developers",
  "license": "https://raw.githubusercontent.com/nodemyadmin/nodemyadmin-native/master/LICENSE",
  "readme": "https://github.com/nodemyadmin/nodemyadmin-native/blob/master/README.md",
  "repository": "https://github.com/nodemyadmin/nodemyadmin-native",
  "nativescript": {
    "id": "org.nativescript.nodemyadmin",
    "tns-android": {
      "version": "2.4.1"
    }
  },
  "scripts": {
    "add:droid": "tns platform add android",
    "remove:droid": "tns platform remove android",
    "start:droid": "tns run android --emulator",
    "start:live:droid": "tns livesync android --emulator --watch",
    "clean:droid": "rimraf ./platforms/android",
    "update:droid": "tns platform update android",
    "lint:all": "npm run lint:web && npm run lint:droid",
    "lint:droid": "tslint -c tslint.json 'app/**/*.ts'",
    "test:droid": "tns test android --emulator",
    "debug:droid": "tns debug android --debug-brk --log trace",
    "bundle:droid": "tns prepare android --bundle",
    "build:droid": "tns build android",
    "deploy:droid": "tns deploy android",
    "check:config": "tns doctor"
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/router": "3.2.1",
    "nativescript-angular": "1.2.0",
    "nativescript-theme-core": "^0.2.1",
    "reflect-metadata": "~0.1.8",
    "rxjs": "5.0.0-beta.12",
    "tns-core-modules": "2.4.1"
  },
  "devDependencies": {
    "babel-traverse": "6.19.0",
    "babel-types": "6.19.0",
    "babylon": "6.14.1",
    "lazy": "1.0.11",
    "nativescript-dev-android-snapshot": "^0.*.*",
    "nativescript-dev-typescript": "^0.3.2",
    "rimraf": "^2.5.4",
    "tslint": "^4.0.2",
    "typescript": "^2.0.10",
    "zone.js": "~0.6.21"
  }
}